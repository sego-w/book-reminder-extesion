{
    "manifest_version": 2,
    "name": "Book Extension",
    "version": "1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup/popup.html"
    },
    "icons" : {
        "16": "images/like.png"
    }
}