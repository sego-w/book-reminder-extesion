{
    "manifest_version": 3,
    "name": "Book Extension",
    "description": "A little widget that reminds you when it's time to get to reading",
    "version": "1",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "icons" : {
        "16": "images/like.png"
    }
}